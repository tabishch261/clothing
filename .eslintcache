pDispatchToProps = dispatch => ({ \n\n  // setCurrentUser goes to the function that gets the user object, then calls dispatch which dispath the action object\n  // dispatch get the action object that i pass to root reducer\n    setCurrentUser: user => dispatch(setCurrentUser(user))\n  \n  }); \n\n// CreatestructuredSelector will automatically pass our top level state that we get as our mapstatetoprop into each subsequent selector\nconst mapStateToProps = createStructuredSelector({\n  currentUser: selectCurrentUser,\n\n});\n\n\n\n// we pass 1st argument as null if we dont need any state to props from our reducer to currentuser here\n// The 2n d argument is the mapDispatchToProps\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(App);",["162","163"],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\pages\\homepage\\homepagecomponent.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\directory\\directory.component.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\menu-item\\menu-item.component.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\pages\\shop\\shop.component.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\preview-collection\\collection-preview.component.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\collection-items\\collection-item.component.jsx",["164"],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\header\\header.component.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\pages\\sign-in-and-sign-up\\sign-in-and-sign-up.component.jsx",[],["165","166"],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\sign-in\\sign-in.component.jsx",[],"C:\\Users\\CHAUDM\\Desktop\\React Developer\\crwn-clothing\\src\\components\\form-input\\form-input.component.jsx",[],["167","168"],"C